<div class="container" style="margin-top: 1em ;">
    <nav class="navbar navbar-dark bg-dark">
        <div class="container-fluid">
            <a class="navbar-brand">User Interface</a>
            <a href="/api/users/logout"
                style="color: #fff; text-decoration: none; background: #e92626; padding: 5px 10px; border-radius: 50%">Logout</a>

        </div>
    </nav>
</div>


<div class="container">
    <h2 style="margin-bottom: 1em; margin-top: 1em">Welcome {{member}}</h2>
    <table class="table table-striped table-bordered">
        <thead class="table-dark">
            <tr>
                <th>Name</th>
                <th>Username</th>
                <th>Email</th>
                {{!-- <th scope="col">Edit</th>
                <th scope="col">Delete</th> --}}
            </tr>
        </thead>
        <tbody>
            {{#each users}}
            <tr class="table-info">
                <td>{{this.name}}</td>
                <td>{{this.username}}</td>
                <td>{{this.email}}</td>
                {{!-- <td><a href="/api/users/update/{{this._id}}"><i style="cursor: pointer"
                            class="fa-solid fa-pen-to-square"></i></a></td>
                <td><a href="/api/users/delete/{{this._id}}"><i style="cursor: pointer"
                            class="fa-solid fa-trash"></i></a>
                </td> --}}
            </tr>
            {{/each}}
        </tbody>
    </table>
</div>